// simple calculator
// https://www.codewars.com/kata/5810085c533d69f4980001cf/train/javascript

function calculator(a,b,sign){
if (typeof a === 'string' || typeof b === 'string'){
  return "unknown value";
} else if (sign === "+"){
  return a + b;
} else if (sign === "-"){
  return a - b;
} else if (sign === "*"){
  return a * b;
} else if (sign === "/"){
  return a / b;
}
 return "unknown value";
}
