// Sum of Multiples
// https://www.codewars.com/kata/57241e0f440cd279b5000829/train/javascript

function sumMul(n, m){
  if (n === 0 && m === 0 || m < 0){
    return "INVALID";
    }
      let sum = 0;
        for (let i = 1; i < m; i++){
        if (i % n === 0 || i % m === 0){ 
      sum = sum + i;
      }
    }
  return sum;
}
