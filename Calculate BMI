// Calculate BMI
// https://www.codewars.com/kata/57a429e253ba3381850000fb

function bmi(weight, height) {
  let res = weight / Math.pow(height, 2);
if (res <= 18.5){
  return "Underweight";
} else if (res <= 25.0){
  return "Normal";
} else if (res <= 30.0){
  return "Overweight";
}
  return "Obese";
}
